    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "helmet": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^3.2.1",
