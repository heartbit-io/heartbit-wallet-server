{
  "name": "heartbit-wallet-server",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "v18.15.0"
  },
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "npx nodemon",
    "build": "concurrently \"rimraf ./build\" \"npx tsc\"",
    "lint": "eslint . --ext .ts",
    "start": "node build/index.js",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "db:migrate": "npm run typeorm migration:run -- -d ./src/domains/repo/index.ts",
    "undo:migration": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate-seed": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "test": "export NODE_ENV=test && mocha -r ts-node/register test/**/*.test.ts",
    "db:test:migrate": "npx sequelize-cli db:migrate --env test",
    "typeorm": "npx typeorm-ts-node-commonjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.6.6",
    "@types/slack-node": "^0.1.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.14",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "chai": "^4.3.7",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@sentry/node": "^7.53.1",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "deepl": "^1.0.13",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "firebase": "^9.20.0",
    "firebase-admin": "^11.6.0",
    "helmet": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "openai": "^3.2.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.30.0",
    "slack-node": "^0.1.8",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.16",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.8.2"
  }
}
