{
  "name": "forum",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "npx nodemon",
    "build": "concurrently \"rimraf ./build\" \"npx tsc -w\"",
    "lint": "eslint . --ext .ts",
    "start": "concurrently \"npm run build\" \"node build/index.js\"",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "db:migrate": "cd sequelize && npx sequelize-cli db:migrate && cd ..",
    "undo:migration": "cd sequelize && npx sequelize-cli db:migrate:undo:all && cd ..",
    "db:seed": "cd sequelize && npx sequelize-cli db:seed:all && cd ..",
    "db:migrate-seed": "cd sequelize && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all && cd .."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
